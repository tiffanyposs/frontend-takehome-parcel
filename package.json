    "concurrently": "^4.0.1",
    "express": "^4.16.3",
    "parcel": "^1.9.7",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-fontawesome": "^1.6.1",
    "react-number-format": "^4.0.6",
